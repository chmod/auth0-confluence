$webResourceManager.requireResourcesForContext("auth0-base-resources")
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>Sign In</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
  <style>
    html, body { padding: 0; margin: 0; }

    .table {
      display: table;
      position: absolute;
      height: 100%;
      width: 100%;
      background: linear-gradient(rgba(255, 255, 255, 0.3), rgba(255, 255, 255, 0));
    }

    .table-row {
      display: table-row;
      vertical-align: middle;
    }

    .content {
      padding: 25px 0px 25px 0px;
      margin-left: auto;
      margin-right: auto;
      width: 280px; /* login widget width */
    }
    
    /* The 'Last-Login-With' button */
    #a0-lock.a0-theme-default .a0-zocial.a0-guest{
      background-color: #00952B !important;
    }
   
    /* The 'Access' button */
    #a0-lock.a0-theme-default .a0-panel button.a0-primary{
      background-color: #00952B !important;
    }
    
    /* The 'Access' button on hover */
    #a0-lock.a0-theme-default .a0-panel button.a0-primary:hover{
      background-color: #00c83f !important;
    }
    
    /* The little red man in the logo */
    #a0-lock.a0-theme-default .a0-panel .a0-icon-container .a0-avatar i{
      display:none !important;
    }
    
   /* The auth0 logo at the bottom*/
   #a0-lock.a0-theme-default .a0-panel .a0-footer .a0-logo i{
     display:none;
   }
   
   /* Remove the 'Access' text from the button */
   #a0-lock.a0-theme-default .a0-panel button.a0-primary {
     line-height: 0px;
     color: transparent!important; 
   }
   
    /* Add 'Log In' to the button instead */
   #a0-lock.a0-theme-default .a0-panel button.a0-primary:after {
     content: 'Sign In' !important;
     display:inherit;
     line-height: initial;
     color: white; 
   }
    
    
  </style>
</head>
<body>
	<div class="table">
		<div class="rable-row">
			<div class="content">
				<!-- WIDGET -->
				<div id="auth0-login"></div>
			</div>
		</div>
		<div class="table-row">
			<div class="content">
				<!-- WIDGET -->
				<div id="local-login">
					Or login locally <a href="/loginlocal.action">here</a>
				</div>
			</div>
		</div>
	</div>

  <!--[if IE 8]>
  <script src="//cdnjs.cloudflare.com/ajax/libs/ie8/0.2.5/ie8.js"></script>
  <![endif]-->

  <!--[if lte IE 9]>
  <script src="https://cdn.auth0.com/js/base64.js"></script>
  <script src="https://cdn.auth0.com/js/es5-shim.min.js"></script>
  <![endif]-->

  <script src="https://cdn.auth0.com/js/lock-9.1.js"></script>

  <script>
	// Decode utf8 characters properly
	var callbackURL="${baseURL}${config.callbackUrl}";
	var osDestination=jQuery.urlParam("os_destination");
	if(osDestination){
		callbackURL=callbackURL+"?os_destination="+osDestination;
		var currentHref=$("#local-login a").attr("href");
		$("#local-login a").prop("href",currentHref+"?os_destination="+osDestination);
	}


    var lock = new Auth0Lock('${config.clientID}', '${config.domain}');
    lock.show({
		icon:               'data:image/png;base64,
		callbackURL:        callbackURL,
		rememberLastLogin:  true,
		responseType: 		'code',
		popup: false,
		container:          'auth0-login',
		authParams: {
			state: '${state}',
			scope: 'openid user_id name nickname email picture'
		}
    });
  </script>

</body>
</html>
